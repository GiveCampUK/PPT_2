    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PrisonId">
            <Value>=Parameters!PrisonId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  Classes.PrisonId
  ,Classes.TeacherId
  ,Contacts.MailName
  ,Contacts.Address1
  ,Contacts.Address2
  ,Contacts.Address3
  ,Contacts.Town
  ,Contacts.County
  ,Contacts.Postcode
  ,Contacts.Tel
  ,Contacts.Fax
  ,Contacts.EmailAddress
  ,Contacts.PersonType
  ,Classes.ClassDetails
  ,Classes.Notes
  ,Contacts.Type
FROM
  Contacts
  INNER JOIN Classes
    ON Contacts.Id = Classes.TeacherId
WHERE
  Classes.PrisonId = @PrisonId</CommandText>
